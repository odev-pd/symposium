{
    "name": "laravel/laravel",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "repositories": [
        {
            "type": "git",
            "url": "git@github.com:mattstauffer/joindin-client.git"
        },
        {
            "type": "git",
            "url": "git@github.com:mattstauffer/laravel-gravatar.git"
        }
    ],
    "type": "project",
    "require": {
        "algolia/algoliasearch-client-php": "^1.18",
        "bugsnag/bugsnag-laravel": "^2.6",
        "doctrine/dbal": "~2.5",
        "geoip/geoip": "^1.17",
        "guzzlehttp/guzzle": "~5.3|~6.0",
        "intervention/image": "^2.4",
        "laravel/framework": "5.5.*",
        "laravel/passport": "^4.0",
        "laravel/scout": "^3.0",
        "laravel/socialite": "^3.0",
        "laravel/tinker": "^1.0",
        "laravelcollective/html": "5.5.*",
        "maddhatter/laravel-fullcalendar": "^1.2",
        "php": ">=7.0.0",
        "pragmarx/firewall": "^1.0",
        "recaptcha/php5": "^2.0",
        "rexxars/joindin-client": "^0.0.1",
        "thomaswelton/laravel-gravatar": "~1.0",
        "thujohn/twitter": "~2.0",
        "predis/predis": "^1.1",
        "andreasindal/laravel-markdown": "1.1.6",
        "fideloper/proxy": "~3.3"
    },
    "require-dev": {
        "phpunit/phpunit": "~6.0",
        "laravel/browser-kit-testing": "^2.0",
        "mockery/mockery": "~1.0",
        "symfony/dom-crawler": "3.1.*",
        "symfony/css-selector": "3.1.*",
        "filp/whoops": "^2.1",
        "fzaninotto/faker": "^1.7"
    },
    "autoload": {
        "classmap": {
            "1": "app/Http/Controllers/",
            "0": "database/seeds"
        },
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "classmap": [
            "tests"
        ]
    },
    "scripts": {
        "post-install-cmd": [
            "@php artisan clear-compiled",
            "@php artisan optimize"
        ],
        "post-update-cmd": [
            "@php artisan clear-compiled",
            "@php artisan optimize"
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate"
        ],
        "test": "phpunit",
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "optimize-autoloader": true
    },
    "sort-packages": true,
    "optimize-autoloader": true,
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    }
}