{
    "name": "laravel/laravel",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "repositories": [
        {
            "type": "git",
            "url": "git@github.com:mattstauffer/joindin-client.git"
        },
        {
            "type": "git",
            "url": "git@github.com:mattstauffer/laravel-gravatar.git"
        }
    ],
    "type": "project",
    "require": {
        "laravel/framework": "5.4.*",
        "rexxars/joindin-client": "^0.0.1",
        "guzzlehttp/guzzle": "~5.3|~6.0",
        "laravelcollective/html": "5.4.*",
        "doctrine/dbal": "~2.5",
        "thomaswelton/laravel-gravatar": "~1.0",
        "thujohn/twitter": "~2.0",
        "recaptcha/php5": "^2.0",
        "bugsnag/bugsnag-laravel": "^2.6",
        "intervention/image": "^2.4",
        "php": ">=5.6.4",
        "pragmarx/firewall": "^1.0",
        "laravel/scout": "^3.0",
        "algolia/algoliasearch-client-php": "^1.18",
        "laravel/passport": "^4.0",
        "laravel/tinker": "^1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~6.0",
        "laravel/browser-kit-testing": "^2.0",
        "mockery/mockery": "0.9.*",
        "symfony/dom-crawler": "3.1.*",
        "symfony/css-selector": "3.1.*",
        "filp/whoops": "^2.1",
        "fzaninotto/faker": "^1.7"
    },
    "autoload": {
        "classmap": [
            "database",
            "app/Http/Controllers/"
        ],
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
      "classmap": [
          "tests"
      ]
    },
    "scripts": {
        "post-install-cmd": [
            "php artisan clear-compiled",
            "php artisan optimize"
        ],
        "post-update-cmd": [
            "php artisan clear-compiled",
            "php artisan optimize"
        ],
        "post-create-project-cmd": [
            "php artisan key:generate"
        ],
        "test": "phpunit"
    },
    "config": {
        "preferred-install": "dist"
    },
    "sort-packages": true,
    "optimize-autoloader": true
}
