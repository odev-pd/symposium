{
    "name": "laravel/laravel",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "type": "project",
    "require": {
        "algolia/algoliasearch-client-php": "^1.18",
        "andreasindal/laravel-markdown": "~3.0",
        "bugsnag/bugsnag-laravel": "^2.6",
        "doctrine/dbal": "~2.5",
        "geoip/geoip": "^1.17",
        "fideloper/proxy": "^4.0",
        "intervention/image": "^2.4",
        "laravel/framework": "5.6.*",
        "laravel/passport": "^6.0",
        "laravel/scout": "^4.0",
        "laravel/socialite": "^3.2.0",
        "laravel/tinker": "^1.0",
        "laravelcollective/html": "5.6.*",
        "maddhatter/laravel-fullcalendar": "^1.2",
        "php": "^7.1.3",
        "pragmarx/firewall": "~2.0",
        "predis/predis": "^1.1",
        "recaptcha/php5": "^2.0",
        "rexxars/joindin-client": "^0.0.1",
        "thomaswelton/laravel-gravatar": "~1.0",
        "thujohn/twitter": "~2.0"
    },
    "require-dev": {
        "filp/whoops": "^2.0",
        "fzaninotto/faker": "^1.7",
        "laravel/browser-kit-testing": "4.*",
        "mockery/mockery": "^1.0",
        "nunomaduro/collision": "^2.0",
        "phpunit/phpunit": "^7.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        },
        "classmap": [
            "database/seeds",
            "database/factories"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-install-cmd": [
            "@php artisan clear-compiled"
        ],
        "post-update-cmd": [
            "@php artisan clear-compiled"
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover"
        ],
        "test": "phpunit"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
